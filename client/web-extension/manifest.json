{
  "manifest_version": 2,
  "name": "web-extension",
  "version": "1.0",
  "permissions": ["webRequest", "webRequestBlocking", "<all_urls>"],

  "background": {
    "scripts": ["background.js"]
  },
  "web_accessible_resources": ["index.html"],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self';"
}
